{
    "name": "wandu/modelr",
    "description": "Simple ORM.",
    "keyword": ["orm", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Changwan Jun",
            "email": "im@wani.kr"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "wandu/standard": "^0.2",
        "wandu/library": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Wandu\\Modelr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wandu\\Modelr\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
